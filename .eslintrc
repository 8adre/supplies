{
  "extends": [
    "airbnb-base",
    "plugin:jsdoc/recommended"
  ],
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0
  },
  "settings": {
    "jsdoc": {
      "overrideReplacesDocs": true,
      "augmentsExtendsReplacesDocs": true,
      "implementsReplacesDocs": true
    }
  },
  "globals": {
    "referenceParam": "readonly",
    "modifierParam": "readonly",
    "decimalsParam": "readonly",
    "coinParam": "readonly"
  },
  "plugins": [
    "jsdoc"
  ],
  "overrides": [
    {
      "files": [
        "**/*.test.js"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}
